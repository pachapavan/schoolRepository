            <column name="total_hostel_fee" type="numeric"/>
            <column name="hostel_feewave_off" type="numeric"/>
            <column name="total_hostel_fee_paid" type="numeric"/>
            <column name="hostel_fee_paid" type="numeric"/>
            <column name="hostel_feepending" type="numeric"/>
