        <addForeignKeyConstraint baseColumnNames="table_values_id"
                                 baseTableName="tabel_values"
                                 constraintName="fk_tabel_values__table_values_id"
                                 referencedColumnNames="id"
                                 referencedTableName="body"/>
    </changeSet>
</databaseChangeLog>
