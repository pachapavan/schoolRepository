<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity TabelValues.
    -->
    <changeSet id="20211213093924-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="attributes_id"
                                 baseTableName="tabel_values"
                                 constraintName="fk_tabel_values__attributes_id"
                                 referencedColumnNames="id"
